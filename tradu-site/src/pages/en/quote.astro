---
import Layout from "../../layouts/Layout.astro";
import QuoteForm from "../../components/QuoteForm.astro";
import Testimonial from "../../components/Testimonial.astro";
import Faq from "../../components/FaqSection.astro";


import t from "../../i18n/en.json";


const lang = "en";
---

<Layout
  title="Translation Service Quote | TraduFlash"
  description="Request a quote for professional document translation services. Fast, confidential, and accurate translations in English and Spanish."
  lang={lang}
  canonical="https://traduflashsv.com/en/quote"
  t={t}
>
  <QuoteForm lang={lang} t={t} />
  <Testimonial lang={lang} t={t} />
  <Faq lang={lang} t={t} />

</Layout>