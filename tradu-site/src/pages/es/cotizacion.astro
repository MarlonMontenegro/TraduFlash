---
import Layout from "../../layouts/Layout.astro";
import QuoteForm from "../../components/QuoteForm.astro";
import Testimonial from "../../components/Testimonial.astro";
import Faq from "../../components/FaqSection.astro";


import t from "../../i18n/es.json";


const lang = "es";
---

<Layout
  title="Cotización de traducción de documentos | TraduFlash"
  description="Solicitá una cotización para la traducción de documentos legales y profesionales. Atención rápida, confidencial y en español e inglés."
  lang={lang}
  canonical="https://traduflashsv.com/es/cotizacion"
  t={t}
>
  <QuoteForm lang={lang} t={t} />
  <Testimonial lang={lang} t={t} />
  <Faq lang={lang} t={t} />

</Layout>